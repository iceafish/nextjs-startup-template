{
  "name": "@relia-fe/core",
  "version": "0.1.0",
  "description": "Shared core utilities, providers, and hooks for startup projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'ESLint not configured for core package'",
    "lint:errors-only": "echo 'ESLint not configured for core package'",
    "lint:fix": "echo 'ESLint not configured for core package'"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.85.6",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "radash": "^12.1.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
